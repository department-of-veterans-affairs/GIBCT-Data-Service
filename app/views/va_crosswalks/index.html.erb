<%= render partial: 'introduction', 
    locals: { 
      new_link: true, edit_link: false, 
      list_link: false, destroy_link: false
    } 
%>

<h2><small>Crosswalk Data</small></h2>

<small>
<div class="row">
  <div class="col-xs-12">
    <table class="table table-hover table-condensed table-bordered table-responsive">
      <thead>
        <th>Id</th>
        <th>Facility Code</th>
        <th>Institution</th>
        <th>OPE</th>
        <th>Cross</th>
        <th colspan="3">Actions</th>
      </thead>
      <tbody>
        <%= render partial: "va_crosswalk", collection: @va_crosswalks, as: :va_crosswalk %>
      </tbody>
    </table>
  </div>
</div>
</small>

<div class="row">
  <div class="col-xs-offset-3 col-xs-6">
    <div class="digg_pagination">
      <%= will_paginate @va_crosswalks %>
    </div>
  </div>
</div>