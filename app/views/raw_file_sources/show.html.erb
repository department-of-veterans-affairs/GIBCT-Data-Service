<%= render partial: 'introduction', 
		locals: { new_link: false, edit_link: true, list_link: true } %>

<h2><small>Raw File Source: <%= @raw_file_source.name %></small></h2>

<div class="row">
	<div class="col-xs-2">
		<strong>Id: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file_source.id %>
	</div>
</div>
<div class="row">
	<div class="col-xs-2">
		<strong>Name: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file_source.name %>
	</div>
</div>
<div class="row">
	<div class="col-xs-2">
		<strong>Build Order: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file_source.build_order %>
	</div>
</div>