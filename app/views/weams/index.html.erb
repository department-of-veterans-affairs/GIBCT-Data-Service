<%= render partial: 'introduction', 
		locals: { 
			new_link: true, edit_link: false, 
			list_link: false, destroy_link: false
		} 
%>

<h2><small>Weams Data</small></h2>

<div class="row">
	<div class="col-xs-12">
		<table class="table table-hover table-condensed table-bordered table-responsive">
			<thead>
				<th>Id</th>
				<th>Facility Code</th>
				<th>Institution</th>
				<th>City</th>
				<th>State</th>
				<th>Zip</th>
				<th>Country</th>
				<th>Bah</th>
				<th>Accredited</th>
				<th>Poe</th>
				<th>Yr</th>
				<th>OJT</th>
				<th>Correspondence</th>
				<th>Flight</th>
				<th colspan="3">Actions</th>
			</thead>
			<tbody>
				<%= render partial: "weam", collection: @weams, as: :weam %>
			</tbody>
		</table>
	</div>
</div>

<div class="row">
	<div class="col-xs-offset-3 col-xs-6">
		<div class="digg_pagination">
			<%= will_paginate @weams %>
		</div>
	</div>
</div>