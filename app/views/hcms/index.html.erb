<%= render partial: 'introduction', 
    locals: { 
      new_link: true, edit_link: false, 
      list_link: false, destroy_link: false
    } 
%>

<h2><small>Hcm Data</small></h2>

<div class="row">
  <div class="col-xs-12">
    <table class="table table-hover table-condensed table-bordered table-responsive">
      <thead>
        <th>Id</th>
        <th>Institution</th>
        <th>OPE</th>
        <th>City</th>
        <th>State</th>
        <th>Monitor Method</th>
        <th>Reason</th>
        <th colspan="3">Actions</th>
      </thead>
      <tbody>
        <%= render partial: "hcm", collection: @hcms, as: :hcm %>
      </tbody>
    </table>
  </div>
</div>

<div class="row">
  <div class="col-xs-offset-3 col-xs-6">
    <div class="digg_pagination">
      <%= will_paginate @hcms %>
    </div>
  </div>
</div>