<%= render partial: 'introduction', 
		locals: { 
			new_link: false, edit_link: true, 
			list_link: true, download_link: @raw_file.latest?
		} 
%>

<h2><small>Raw File: <%= @raw_file.name %></small></h2>
	
<div class="row">
	<div class="col-xs-2">
		<strong>Id: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file.id %>
	</div>
</div>

<div class="row">
	<div class="col-xs-2">
		<strong>Type: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file.type %>
	</div>
</div>

<div class="row">
	<div class="col-xs-2">
		<strong>Server Name: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file.name %>
	</div>
</div>

<div class="row">
	<div class="col-xs-2">
		<strong>Data Source: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file.raw_file_source.name %>
	</div>
</div>

<div class="row">
	<div class="col-xs-2">
		<strong>Upload Date: </strong>
	</div>
	<div class="col-xs-3">
		<%= @raw_file.upload_date.strftime("%B %d, %Y") %>
	</div>
</div>
