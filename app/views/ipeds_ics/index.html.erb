<%= render partial: 'introduction', 
    locals: { 
      new_link: true, edit_link: false, 
      list_link: false, destroy_link: false
    } 
%>

<h2><small>Mou Data</small></h2>

<div class="row">
  <div class="col-xs-12">
    <table class="table table-hover table-condensed table-bordered table-responsive">
      <thead>
        <th>Id</th>
        <th>Cross</th>
        <th>Vet2</th>
        <th>Vet3</th>
        <th>Vet4</th>
        <th>Vet5</th>
        <th>Calsys</th>
        <th>Distnced</th>
        <th colspan="3">Actions</th>
      </thead>
      <tbody>
        <%= render partial: "ipeds_ic", collection: @ipeds_ics, as: :ipeds_ic %>
      </tbody>
    </table>
  </div>
</div>

<div class="row">
  <div class="col-xs-offset-3 col-xs-8">
    <div class="digg_pagination">
      <%= will_paginate @ipeds_ics %>
    </div>
  </div>
</div>