<h2>Loaded CSVs</h2>

<small>
  <div class="row">
    <div class="col-sm-10">
      <table class="table table-hover table-condensed table-bordered table-responsive">
        <thead>
          <th>Id</th>
          <th>Upload Date</th>
          <th>Uploaded By</th>
          <th>Type</th>
          <th>Result</th>
          <th>File Name</th>
          <th>Description</th>
          <th colspan="3">Actions</th>
        </thead>
        <tbody>
          <%= render partial: "csv_file", collection: @csv_files, as: :csv_file %>
        </tbody>
      </table>
    </div>
    <div class="col-xs-2">
      <%= link_to 'Upload New Csv File', new_csv_file_path, class: 'btn btn-primary', role: 'button'%>
    </div>
  </div>
</small>

<div class="row">
  <div class="col-xs-offset-4 col-sm-8">
    <div class="digg_pagination">
      <%= will_paginate @csv_files %>
    </div>
  </div>
</div>
