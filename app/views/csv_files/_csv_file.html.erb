<% alert = csv_file.result == 'Successful' ? 'alert alert-success' : 'alert alert-danger' %>

<tr>
  <td><%= csv_file.id %></td>
  <td><%= csv_file.created_at.to_formatted_s(:long) %></td>
  <td><%= csv_file.user.email %></td>
  <td><%= csv_file.csv_type %></td>
  <td class="<%= alert %>"><%= csv_file.result %></td>
  <td><%= csv_file.name %></td>
  <td><%= csv_file.description %></td>
  <td><%= link_to 'Show', csv_file %></td>
</tr>
