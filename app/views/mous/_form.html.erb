<%= form_for(@mou) do |f| %>
  <div class="form-group">
    <%= f.label(:institution) %>
    <%= f.text_field :institution, 
      { value: "#{@mou.institution}", class: "form-control", required: true } %>
  </div>

  <div class="form-group">
    <%= f.label(:ope) %>
    <%= f.text_field :ope, 
      { value: "#{@mou.ope}", class: "form-control", required: true } %>
  </div>

  <div class="form-group">
    <%= f.label(:city) %>
    <%= f.text_field :city, 
      { value: "#{@mou.city}", class: "form-control", required: false } %>
  </div>

  <div class="form-group">
    <%= f.label(:state) %>
    <%= f.select(:state, 
      options_for_select(DS::State.get_as_options, 
      selected: @mou.state), 
      { include_blank: "Select a state" },
      { class: "form-control", required: false }) %>
  </div>

  <div class="form-group">
    <%= f.label(:dod_mou) %>
    <%= f.text_field :dod_mou, 
      { value: "#{@mou.dod_mou}", class: "form-control", required: false } %>
  </div>

  <div class="actions">
    <%= link_to "Cancel", csv_files_path, class: "btn btn-default" %>  
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>