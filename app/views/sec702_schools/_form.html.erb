<%= form_for(@sec702_school) do |f| %>
  <div class="form-group">
    <%= f.label(:facility_code) %>
    <%= f.text_field :facility_code, 
      { value: "#{@sec702_school.facility_code}", class: "form-control", required: true } %>
  </div>

  <div class="form-group">
    <%= f.label(:sec_702) %>
    <%= f.select(:sec_702, 
      options_for_select([DS_ENUM::Truth.yes, DS_ENUM::Truth.no], 
      selected: @sec702_school.sec_702), 
      { include_blank: "Select yes or no" },
      { class: "form-control", required: false }) %>
  </div>
    
  <div class="actions">
    <%= link_to "Cancel", csv_files_path, class: "btn btn-default" %>  
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>