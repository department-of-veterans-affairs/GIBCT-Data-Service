<%= form_for(@ipeds_hd) do |f| %>
  <div class="form-group">
    <%= f.label(:cross) %>
    <%= f.text_field :cross, 
      { value: "#{@ipeds_hd.cross}", class: "form-control", required: true } %>
  </div>
  
  <div class="form-group">
    <%= f.label(:veturl) %>
    <%= f.text_field :veturl, 
      { value: "#{@ipeds_hd.veturl}", class: "form-control", required: false } %>
  </div>  

  <div class="actions">
    <%= link_to "Cancel", csv_files_path, class: "btn btn-default" %>  
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>