<%= render partial: 'introduction', 
    locals: { 
      new_link: false, edit_link: true, 
      list_link: true, destroy_link: true
    } 
%>

<h2><small>Veteran Success: <%= @vsoc.institution %></small></h2>
  
<div class="row">
  <div class="col-xs-2">
    <strong>Id: </strong>
  </div>
  <div class="col-xs-8">
    <%= @vsoc.id %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Facility Code: </strong>
  </div>
  <div class="col-xs-8">
    <%= @vsoc.facility_code %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Institution: </strong>
  </div>
  <div class="col-xs-8">
    <%= @vsoc.institution %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Veteran Success Name: </strong>
  </div>
  <div class="col-xs-3">
    <%= @vsoc.vetsuccess_name %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Veteran Success Email: </strong>
  </div>
  <div class="col-xs-3">
    <%= @vsoc.vetsuccess_email %>
  </div>
</div>