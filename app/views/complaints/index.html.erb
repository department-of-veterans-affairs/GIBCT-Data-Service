<%= render partial: 'introduction', 
    locals: { 
      new_link: true, edit_link: false, 
      list_link: false, destroy_link: false
    } 
%>

<h2><small>Complaint</small></h2>

<small>
<div class="row">
  <div class="col-xs-12">
    <table class="table table-hover table-condensed table-bordered table-responsive">
      <thead>
        <th>Id</th>
        <th>Facility Code</th>
        <th>Ope</th>
        <th>Ope 6</th>
        <th>Institution</th>
        <th>Status</th>
        <th>Closed Reason</th>
        <th>Issue</th>
        <th colspan="3">Actions</th>
      </thead>
      <tbody>
        <%= render partial: "complaint", collection: @complaints, as: :complaint %>
      </tbody>
    </table>
  </div>
</div>
</small>

<div class="row">
  <div class="col-xs-offset-2 col-xs-10">
    <div class="digg_pagination">
      <%= will_paginate @complaint %>
    </div>
  </div>
</div>