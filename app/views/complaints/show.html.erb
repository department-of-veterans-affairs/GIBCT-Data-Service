<%= render partial: 'introduction', 
    locals: { 
      new_link: false, edit_link: true, 
      list_link: true, destroy_link: true
    } 
%>

<h2><small>Complaint: <%= @complaint.institution %></small></h2>
  
<div class="row">
  <div class="col-xs-3">
    <strong>Id: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.id %>
  </div>
</div>

<div class="row">
  <div class="col-xs-3">
    <strong>Facility Code: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.facility_code %>
  </div>
</div>

<div class="row">
  <div class="col-xs-3">
    <strong>Ope: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.ope %>
  </div>
</div>

<div class="row">
  <div class="col-xs-3">
    <strong>Ope 6: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.ope6 %>
  </div>
</div>

<div class="row">
  <div class="col-xs-3">
    <strong>Institution: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.institution %>
  </div>
</div>

<div class="row">
  <div class="col-xs-3">
    <strong>Status: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.status %>
  </div>
</div>

<div class="row">
  <div class="col-xs-3">
    <strong>Closed Reason: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.closed_reason %>
  </div>
</div>

<div class="row">
  <div class="col-xs-3">
    <strong>Issue: </strong>
  </div>
  <div class="col-xs-8">
    <%= @complaint.issue %>
  </div>
</div>
