<div class="row row-space-6" style="padding-bottom: 10px">
  <div class="col-xs-12">
    <%= render partial: 'introduction' %>
  </div>
</div>

<div class="row row-space-6" style="margin-left: 4%">
  <div class="col-xs-4">
    <%= render 'form' %>
  </div>
  <% if @requirements&.any? %>
    <div class="col-xs-3" style="margin-left:11%">
      <div class="alert alert-info" role="alert">
        <h4 class="alert-heading">CSV Data Requirements</h4><br />
        <ul>
          <% @requirements.each do |validations| %>
            <li><%= validations %></li>
          <% end %>
        </ul>
      </div>
    </div>
  <% end %>
</div>

<div class="row row-space-6">
  <div class="col-lg-12">
    <%= render 'layouts/upload_messages' %>
  </div>
</div>
