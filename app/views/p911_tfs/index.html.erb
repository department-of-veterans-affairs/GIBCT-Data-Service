<%= render partial: 'introduction', 
    locals: { 
      new_link: true, edit_link: false, 
      list_link: false, destroy_link: false
    } 
%>

<h2><small>P911 TF Data</small></h2>

<div class="row">
  <div class="col-xs-12">
    <table class="table table-hover table-condensed table-bordered table-responsive">
      <thead>
        <th>Id</th>
        <th>Facility Code</th>
        <th>Institution</th>
        <th>Post 911 Tuition Fees</th>
        <th>Post 911 Recipients</th>
        <th colspan="3">Actions</th>
      </thead>
      <tbody>
        <%= render partial: "p911_tf", collection: @p911_tfs, as: :p911_tf %>
      </tbody>
    </table>
  </div>
</div>

<div class="row">
  <div class="col-xs-offset-2 col-xs-10">
    <div class="digg_pagination">
      <%= will_paginate @p911_tf %>
    </div>
  </div>
</div>