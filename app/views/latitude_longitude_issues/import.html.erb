
    <%= form_with do |f| %>

  <div class="form-group">
    <%= f.label(:file_group, for: 'csv_type_display') %><br />
    <%= f.text_field(:csv_type, disabled: true, name: "csv_type_display", id: "csv_type_display", value: @census_lat_long.class.name) %>
    <%= f.hidden_field(:csv_type) %>
  </div>


      <div class="form-group">
        <%= f.label(:skip_lines_before_header, for: "upload_skip_lines") %><br />
        <%= f.text_field(:skip_lines) %>
      </div>

      <div class="form-group">
        <%= f.label(:upload_files) %>
        <p class="help-block">Valid file formats include: <%= @extensions %></p>
        <%= f.file_field(:uploaded_files, class: "form-control", multiple: true, required: true, accept: @extensions) %>
      </div>

      <div class="form-group">
        <%= f.label(:comment) %><br />
        <%= f.text_field(:comment, style: 'width: 100%') %>
      </div>

      <div class="actions">
        <%= link_to "Cancel", uploads_path, class: "btn btn-default" %>
        <%= f.submit "upload", class: "btn btn-primary" %>
      </div>
    <% end %>