<%= render partial: 'introduction', 
    locals: { 
      new_link: true, edit_link: false, 
      list_link: false, destroy_link: false
    } 
%>

<h2><small>Outcome Data</small></h2>

<small>
<div class="row">
  <div class="col-xs-12">
    <table class="table table-hover table-condensed table-bordered table-responsive">
      <thead>
        <th>Id</th>
        <th>Facility Code</th>
        <th>Institution</th>
        <th>Veteran Retention Rate BA</th>
        <th>Veteran Retention Rate Other</th>        
        <th>Veteran Persistance Rate BA</th>
        <th>Veteran Persistance Rate Other</th>
        <th>Veteran Graduation Rate</th>
        <th>Veteran Transfer Out Rate</th>
        <th colspan="3">Actions</th>
      </thead>
      <tbody>
        <%= render partial: "outcome", collection: @outcomes, as: :outcome %>
      </tbody>
    </table>
  </div>
</div>
</small>

<div class="row">
  <div class="col-xs-offset-2 col-xs-10">
    <div class="digg_pagination">
      <%= will_paginate @outcome %>
    </div>
  </div>
</div>