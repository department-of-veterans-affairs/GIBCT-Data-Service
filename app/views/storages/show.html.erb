<div class="row row-space-6" style="padding-bottom: 10px">
  <div class="col-xs-12">
    <%= render partial: 'introduction' %>
  </div>
</div>

<div class="row row-space-6">
  <div class="col-xs-2">
    <strong>Id: </strong>
  </div>
  <div class="col-xs-3">
    <%= @storage.id %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Type: </strong>
  </div>
  <div class="col-xs-3">
    <%= @storage.csv_type %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Source File: </strong>
  </div>
  <div class="col-xs-3">
    <%= @storage.csv %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Uploaded By: </strong>
  </div>
  <div class="col-xs-3">
    <%= @storage.user.email %>
  </div>
</div>

<div class="row">
  <div class="col-xs-2">
    <strong>Uploaded On: </strong>
  </div>
  <div class="col-xs-3">
    <%= @storage.created_at.strftime('%b %e, %Y %H:%M:%S') %>
  </div>
</div>

<div class="row" style: "padding-bottom: 20px">
  <div class="col-xs-2">
    <strong>Comments: </strong>
  </div>
  <div class="col-xs-3">
    <%= @storage.comment %>
  </div>
</div>
