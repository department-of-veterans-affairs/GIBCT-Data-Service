<%= form_for(@ipeds_ic_ay) do |f| %>
  <div class="form-group">
    <%= f.label(:cross, 'Ipeds') %>
    <%= f.text_field :cross, 
      { value: "#{@ipeds_ic_ay.cross}", class: "form-control", required: true } %>
  </div>

  <div class="form-group">
    <%= f.label(:tuition_in_state) %>
    <%= f.text_field :tuition_in_state, 
      { value: "#{@ipeds_ic_ay.tuition_in_state}", class: "form-control", required: false } %>
  </div>

  <div class="form-group">
    <%= f.label(:tuition_out_of_state) %>
    <%= f.text_field :tuition_out_of_state, 
      { value: "#{@ipeds_ic_ay.tuition_out_of_state}", class: "form-control", required: false } %>
  </div>  

  <div class="form-group">
    <%= f.label(:books) %>
    <%= f.text_field :books, 
      { value: "#{@ipeds_ic_ay.books}", class: "form-control", required: false } %>
  </div>

  <div class="actions">
    <%= link_to "Cancel", csv_files_path, class: "btn btn-default" %>  
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>