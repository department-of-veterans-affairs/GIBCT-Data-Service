<%= form_for(@arf_gibill) do |f| %>
  <div class="form-group">
    <%= f.label(:facility_code) %>
    <%= f.text_field :facility_code, 
      { value: "#{@arf_gibill.facility_code}", class: "form-control", required: true } %>
  </div>

  <div class="form-group">
    <%= f.label(:institution) %>
    <%= f.text_field :institution, 
      { value: "#{@arf_gibill.institution}", class: "form-control", required: false } %>
  </div>

  <div class="form-group">
    <%= f.label(:gibill, "Total Students") %>
    <%= f.text_field :gibill, 
      { value: "#{@arf_gibill.gibill}", class: "form-control", required: false } %>
  </div>
  
  <div class="actions">
    <%= link_to "Cancel", csv_files_path, class: "btn btn-default" %>  
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>