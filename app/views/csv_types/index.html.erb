<%= render partial: 'introduction', 
    locals: { new_link: false, edit_link: false, list_link: false } %>

<h2><small>Currently Defined CSV File Types</small></h2>

<div class="row">
  <div class="col-xs-8">
    <table class="table table-hover table-condensed table-bordered table-responsive">
      <thead>
        <th>Name</th>
        <th>Last Uploaded CSV File</th>
        <th>Upload Date</th>
        <th colspan="1">Actions</th>
      </thead>
      <tbody>
        <%= render partial: 'csv_type', collection: @csv_types %>
      </tbody>
    </table>
  </div>
</div>