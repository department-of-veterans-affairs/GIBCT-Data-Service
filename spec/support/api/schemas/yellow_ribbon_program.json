{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "meta": {
      "type": "object",
      "properties": {
        "count": {
          "type" : "integer"
        },
        "version": {
          "type": "object",
          "properties": {
            "number": {
              "type": "number"
            },
            "created_at": {
              "type": "string"
            },
            "preview": {
              "type": "boolean"
            }
          }
        }
      },
      "required": ["version"]
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "maxItems": 10,
        "additionalProperties": false,
        "properties": {
          "id": { "type": "string" },
          "type": { "type": "string" },
          "attributes": {
            "type": "object",
            "properties": {
              "city": { "type": ["null", "string"] },
              "contribution_amount": { "type": ["null", "string"] },
              "degree_level": { "type": ["null", "string"] },
              "division_professional_school": { "type": ["null", "string"] },
              "number_of_students": { "type": ["null", "integer"] },
              "school_name_in_yr_database": { "type": ["null", "string"] },
              "state": { "type": ["null", "string"] },
              "street_address": { "type": ["null", "string"] },
              "zip": { "type": ["null", "string"] }
            },
            "required": [
              "school_name_in_yr_database"
            ]
          }
        },
        "required": ["id", "type", "attributes"]
      }
    },
    "links": {
      "type": "object",
      "properties": {
        "self": { "type": "string" },
        "first": { "type": "string" },
        "prev": { "type": ["null", "string"] },
        "next": { "type": ["null", "string"] },
        "last": { "type": "string" }
      },
      "required": ["self", "first", "prev", "next", "last"]
    }
  },
  "required": ["meta", "data", "links"]
}
